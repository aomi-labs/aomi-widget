{
  "name": "aomi-widget",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for the Aomi React runtime, registry, and documentation.",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter landing dev",
    "dev:landing:live": "pnpm run build:react -- --watch & pnpm --filter landing dev",
    "dev:docs": "pnpm --filter @aomi-labs/widget-docs dev",
    "build": "pnpm run build:react && pnpm --filter registry run build",
    "build:react": "pnpm --filter @aomi-labs/react run build",
    "build:docs": "pnpm --filter @aomi-labs/widget-docs build",
    "lint": "eslint .",
    "lint:docs": "pnpm --filter @aomi-labs/widget-docs lint",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "generate:theme": "node ./scripts/generate-theme.mjs"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "tailwindStylesheet": "app/globals.css"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^2.0.46",
    "@assistant-ui/react": "^0.11.28",
    "@assistant-ui/react-ai-sdk": "^1.1.5",
    "@assistant-ui/react-markdown": "^0.11.1",
    "@eslint/eslintrc": "^3",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@reown/appkit": "^1.8.14",
    "@reown/appkit-adapter-wagmi": "^1.8.14",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query": "^5.90.10",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "ai": "^5.0.65",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.545.0",
    "motion": "^12.23.22",
    "next": "15.5.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-shiki": "^0.9.0",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4",
    "tsup": "^8.5.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "viem": "^2.40.3",
    "wagmi": "^2.19.5",
    "zustand": "^5.0.8"
  }
}
